<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent" android:background="#ffffffff">
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:background="#ffffffff">
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="190dp"
                        android:id="@+id/imageButton8" android:layout_weight="1" android:layout_marginLeft="10dp"
                        android:layout_marginTop="2dp" android:layout_marginRight="2dp" android:layout_marginBottom="2dp"
                        android:background="@drawable/p2221352090"/>
                <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="190dp"
                        android:id="@+id/imageButton9" android:layout_weight="1" android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp" android:layout_marginRight="10dp" android:layout_marginBottom="2dp"
                        android:background="@drawable/p2221351999"/>
            </LinearLayout>
            <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">
                <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="190dp"
                        android:id="@+id/imageButton10" android:layout_weight="1" android:layout_marginLeft="10dp"
                        android:layout_marginTop="2dp" android:layout_marginRight="2dp" android:layout_marginBottom="2dp"
                        android:background="@drawable/p2221353377"/>
                <ImageButton
                        android:layout_width="wrap_content"
                        android:layout_height="190dp"
                        android:id="@+id/imageButton11" android:layout_weight="1" android:layout_marginLeft="2dp"
                        android:layout_marginTop="2dp" android:layout_marginRight="10dp" android:layout_marginBottom="2dp"
                        android:background="@drawable/p2221352087"/>
            </LinearLayout>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Fashio"
                    android:id="@+id/textView2" android:layout_marginLeft="10dp" android:layout_marginTop="2dp"
                    android:textColor="#af949494"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/Lolita"
                    android:id="@+id/textView3" android:layout_marginLeft="10dp" android:layout_marginTop="2dp"
                    android:textColor="#af949494" android:singleLine="false" android:layout_marginRight="10dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Jane"
                    android:id="@+id/textView4" android:layout_marginRight="10dp"
                    android:layout_marginTop="2dp" android:layout_gravity="right" android:layout_marginBottom="15dp"/>
        </LinearLayout>
</LinearLayout>
