<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent" android:background="#ffffff">

    <LinearLayout
            android:id="@+id/travelLinearLayout"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:layout_marginTop="25dp">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent"
                      android:layout_height="match_parent" android:layout_weight="1" android:layout_marginTop="5dp"
                      android:layout_marginLeft="20dp" android:layout_marginRight="20dp">
            <ImageView android:layout_width="match_parent" android:layout_height="match_parent"
                       android:id="@+id/imageView24" android:layout_weight="1" android:layout_marginRight="2dp"
                       android:scaleType="fitXY" android:src="@drawable/travel_g"/>
            <ImageView android:layout_width="match_parent" android:layout_height="match_parent"
                       android:id="@+id/imageView25" android:layout_weight="1" android:layout_marginLeft="2dp"
                       android:src="@drawable/travel_h" android:scaleType="fitXY"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent"
                      android:layout_height="match_parent" android:layout_weight="1" android:layout_marginTop="5dp"
                      android:layout_marginLeft="20dp" android:layout_marginRight="20dp">
            <ImageView android:layout_width="match_parent" android:layout_height="match_parent"
                       android:id="@+id/imageView22" android:layout_weight="1" android:layout_marginRight="2dp"
                       android:scaleType="fitXY" android:src="@drawable/travel_e"/>
            <ImageView android:layout_width="match_parent" android:layout_height="match_parent" android:id="@+id/imageView23"
                       android:layout_weight="1" android:layout_marginLeft="2dp" android:src="@drawable/travel_f"
                       android:scaleType="fitXY"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_weight="1" android:layout_marginTop="5dp"
                android:layout_marginLeft="20dp" android:layout_marginRight="20dp">
            <ImageView android:layout_width="match_parent" android:layout_height="match_parent"
                       android:id="@+id/imageView20" android:layout_weight="1" android:layout_marginRight="2dp"
                       android:scaleType="fitXY" android:src="@drawable/travel_c"/>
            <ImageView android:layout_width="match_parent" android:layout_height="match_parent"
                       android:id="@+id/imageView21" android:layout_weight="1" android:layout_marginLeft="2dp"
                       android:src="@drawable/travel_d" android:scaleType="fitXY"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:layout_weight="1" android:layout_marginTop="5dp"
                android:layout_marginLeft="20dp" android:layout_marginRight="20dp">
            <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/imageView17" android:layout_weight="1" android:layout_marginRight="2dp"
                    android:src="@drawable/travel_a" android:scaleType="fitXY"/>
            <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/imageView18" android:layout_weight="1" android:layout_marginLeft="2dp"
                    android:scaleType="fitXY" android:src="@drawable/travel_b"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent"
                      android:layout_height="match_parent" android:layout_weight="1">
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="@string/travel_title"
                    android:id="@+id/textView19" android:layout_gravity="center" android:layout_marginTop="20dp"
                    android:textSize="30sp" android:textColor="#33000000"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>